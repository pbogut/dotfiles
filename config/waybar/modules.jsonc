    "custom/shadowplay": {
        "format": "<span color=\"#bd2c40\">{icon} {}</span>",
        "interval": 360,
        "signal": 15,
        "exec": "pgrep wl-screenrec -a | sed 's,.* -o \\([^ ]*\\).*,\\1,'",
        "format-icons": {
            "default": "ï¥‰"
        },
    },
    "custom/notmuch": {
        "format": "<span color=\"#2e9ef4\">{icon}</span>  {}",
        "interval": 60,
        "signal": 25,
        "exec": "c() { u=\"$(notmuch count -- tag:unread and tag:inbox)\"; i=\"$(notmuch count --  tag:inbox)\"; if [[ $u != \"0\" ]]; then echo $u/$i; else echo \"\"; fi }; c",
        "on-click": "email",
        "format-icons": {
            "default": "ïƒ "
        },
    },
    "custom/memory_left": {
        "format": "<span color=\"#2e9ef4\">{icon}</span>  {}",
        "interval": 30,
        "exec": "free -h | grep 'Mem' | awk '{print $7 \" (\"$4\")\"}'",
        "format-icons": {
            "default": "ï‡¾"
        },
    },
